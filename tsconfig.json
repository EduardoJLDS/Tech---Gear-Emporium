{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "types": [
      "vite/client",
      "vitest/globals"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "/.",
  }
}